{
  "name": "khoan-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf node_modules/.vite dist",
    "clean-all": "rm -rf node_modules dist && npm install",
    "format": "prettier --write \"src/**/*.{vue,js,ts,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{vue,js,ts,json,css,md}\"",
    "type-check": "vue-tsc --noEmit",
    "type-check:advanced": "bash ./check-types.sh",
    "serve": "vite preview",
    "analyze": "vite build --mode analyze",
    "lint": "eslint --ext .js,.ts,.vue --fix src/",
    "lint:check": "eslint --ext .js,.ts,.vue src/",
    "fix-warnings": "bash ./fix-warnings.sh",
    "quality-check": "npm run lint && npm run type-check && npm run fix-warnings",
    "prepare-prod": "./prepare-for-production.sh",
    "dev-help": "./dev-assistant.sh",
    "code-report": "./code-quality-report.sh",
    "component-guide": "./component-guide.sh"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/motion": "^3.0.3",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.10.2",
    "node-fetch": "^3.3.2",
    "pinia": "^3.0.2",
  "xlsx": "^0.18.5",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vue-tsc": "^2.2.10",
    "workbox-window": "^7.3.0"
  }
}
