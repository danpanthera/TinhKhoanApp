{
  "name": "tinhkhoan-app-ui-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf node_modules/.vite dist",
    "clean-all": "rm -rf node_modules dist && npm install",
    "format": "prettier --write \"src/**/*.{vue,js,ts,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{vue,js,ts,json,css,md}\"",
    "type-check": "vue-tsc --noEmit",
    "serve": "vite preview",
    "analyze": "vite build --mode analyze"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/motion": "^3.0.3",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.10.2",
    "lottie-vuejs": "^0.4.0",
    "node-fetch": "^3.3.2",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vue-tsc": "^2.2.10",
    "workbox-window": "^7.3.0"
  }
}
